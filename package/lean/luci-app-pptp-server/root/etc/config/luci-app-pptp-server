
config service 'pptpd'
	option mppe '1'
	option localip '192.168.8.200'
	option remoteip '192.168.8.180-190'
	option enabled '1'

config users
	option enabled '1'
	option username 'myvpn'
	option password 'myvpn'

config users
	option username 'myvpn1'
	option password 'myvpn1'
	option enabled '1'

config users
	option enabled '1'
	option username 'myvpn2'
	option password 'myvpn2'